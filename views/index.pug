doctype html
html(lang='en')
    head
        // Required meta tags
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        title Star Admin Free Bootstrap-4 Admin Dashboard Template
        // plugins:css
        link(rel='stylesheet', href='node_modules/mdi/css/materialdesignicons.min.css')
        link(rel='stylesheet', href='node_modules/simple-line-icons/css/simple-line-icons.css')
        // endinject
        // plugin css for this page
        // End plugin css for this page
        // inject:css
        link(rel='stylesheet', href='/stylesheets/css/style.css')
        // endinject
        link(rel='shortcut icon', href='images/favicon.png')
    body
        .container-scroller
            // partial:partials/_navbar.html
            nav.navbar.col-lg-12.col-12.p-0.fixed-top.d-flex.flex-row
                .text-center.navbar-brand-wrapper.d-flex.align-items-top.justify-content-center
                    a.navbar-brand.brand-logo(href='/admin')
                        img(src='images/logo.svg', alt='logo')
                    a.navbar-brand.brand-logo-mini(href='/admin')
                        img(src='images/logo-mini.svg', alt='logo')
                .navbar-menu-wrapper.d-flex.align-items-center
                    ul.navbar-nav.navbar-nav-left.header-links.d-none.d-md-flex
                        li.nav-item
                            a.nav-link(href='#')
                                i.mdi.mdi-image-filter
                                | #{title}
                    ul.navbar-nav.navbar-nav-right
                        li.nav-item.dropdown
                            a#notificationDropdown.nav-link.count-indicator.dropdown-toggle(href='#', data-toggle='dropdown')
                                i.mdi.mdi-bell-ring
                                span.count 4
                            .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='notificationDropdown')
                                a.dropdown-item
                                    p.mb-0.font-weight-normal.float-left
                                        | You have 4 new notifications
                                    span.badge.badge-pill.badge-warning.float-right View all
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        .preview-icon.bg-success
                                            i.icon-info.mx-0
                                    .preview-item-content
                                        h6.preview-subject.font-weight-medium Application Error
                                        p.font-weight-light.small-text
                                            | Just now
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        .preview-icon.bg-warning
                                            i.icon-speech.mx-0
                                    .preview-item-content
                                        h6.preview-subject.font-weight-medium Settings
                                        p.font-weight-light.small-text
                                            | Private message
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        .preview-icon.bg-info
                                            i.icon-envelope.mx-0
                                    .preview-item-content
                                        h6.preview-subject.font-weight-medium New user registration
                                        p.font-weight-light.small-text
                                            | 2 days ago
                        li.nav-item.dropdown
                            a#messageDropdown.nav-link.count-indicator.dropdown-toggle(href='#', data-toggle='dropdown', aria-expanded='false')
                                i.mdi.mdi-email-variant
                                span.count 7
                            .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='messageDropdown')
                                .dropdown-item
                                    p.mb-0.font-weight-normal.float-left
                                        | You have 7 unread mails
                                    span.badge.badge-info.badge-pill.float-right View all
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        img.profile-pic(src='images/faces/face4.jpg', alt='image')
                                    .preview-item-content.flex-grow
                                        h6.preview-subject.ellipsis.font-weight-medium
                                            | David Grey
                                            span.float-right.font-weight-light.small-text 1 Minutes ago
                                        p.font-weight-light.small-text
                                            | The meeting is cancelled
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        img.profile-pic(src='images/faces/face2.jpg', alt='image')
                                    .preview-item-content.flex-grow
                                        h6.preview-subject.ellipsis.font-weight-medium
                                            | Tim Cook
                                            span.float-right.font-weight-light.small-text 15 Minutes ago
                                        p.font-weight-light.small-text
                                            | New product launch
                                .dropdown-divider
                                a.dropdown-item.preview-item
                                    .preview-thumbnail
                                        img.profile-pic(src='images/faces/face3.jpg', alt='image')
                                    .preview-item-content.flex-grow
                                        h6.preview-subject.ellipsis.font-weight-medium
                                            | Johnson
                                            span.float-right.font-weight-light.small-text 18 Minutes ago
                                        p.font-weight-light.small-text
                                            | Upcoming board meeting
                        li.nav-item.d-none.d-lg-block
                            a.nav-link(href='#')
                                img.img-xs.rounded-circle(src='images/faces/face4.jpg', alt='')
                    button.navbar-toggler.navbar-toggler-right.d-lg-none.align-self-center(type='button', data-toggle='offcanvas')
                        span.icon-menu
            // partial
            .container-fluid.page-body-wrapper
                // partial:partials/_sidebar.html
                nav#sidebar.sidebar.sidebar-offcanvas
                    ul.nav
                        li.nav-item.nav-profile
                            .nav-link
                                .profile-image
                                    img(src='images/faces/face4.jpg', alt='image')
                                    span.online-status.online
                                .profile-name
                                    p.name #{name_user}
                                    p.designation #{status}
                                    .badge.badge-teal.mx-auto.mt-3 Online
                        li.nav-item
                            a.nav-link(href='/users')
                                img.menu-icon(src='images/menu_icons/01.png', alt='menu icon')
                                span.menu-title Позльзователи
                        li.nav-item
                            a.nav-link(href='/country')
                                img.menu-icon(src='images/menu_icons/02.png', alt='menu icon')
                                span.menu-title Список стран
                        li.nav-item
                            a.nav-link(href='/city')
                                img.menu-icon(src='images/menu_icons/03.png', alt='menu icon')
                                span.menu-title Список городов
                        li.nav-item
                            a.nav-link(href='/baseFine')
                                img.menu-icon(src='images/menu_icons/04.png', alt='menu icon')
                                span.menu-title База штрафов
                // partial
                .main-panel
                    .content-wrapper
                        .row.purchace-popup

                    // content-wrapper ends
                    // partial:partials/_footer.html
                    footer.footer
                        .container-fluid.clearfix
                    // partial
                // main-panel ends
            // page-body-wrapper ends
        // container-scroller
        // plugins:js
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
        script(src='node_modules/popper.js/dist/umd/popper.min.js')
        script(src='node_modules/bootstrap/dist/js/bootstrap.min.js')
        // endinject
        // Plugin js for this page
        script(src='/javascripts/js/chart.js')
        // End plugin js for this page
        // inject:js
        script(src='/javascripts/js/off-canvas.js')
        script(src='/javascripts/js/misc.js')
        // endinject
        // Custom js for this page
        script(src='/javascripts/js/dashboard.js')
        script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyB5NXz9eVnyJOA81wimI8WYE08kW_JMe8g&callback=initMap', async='', defer='')
        script(src='/javascripts/js/maps.js')
        // End custom js for this page
